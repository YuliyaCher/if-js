{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-use-before-define": 0,
        "func-names": "off",
        "semi": 2,
        "linebreak-style": ["error", "windows"],
        "no-plusplus": "off",
        "no-shadow": "off",
        "prefer-const": "off",
        "prefer-template":"off",
        "nonblock-statement-body-position":"off",
        "curly":"off",
        "consistent-return":"off",
        "operator-linebreak":"off",
        "no-restricted-syntax":"off"
        }
}
