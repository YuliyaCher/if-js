{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-use-before-define": 0,
        "func-names": "off",
        "semi": ["never"],
        "linebreak-style": ["error", "windows"]
    }
}
